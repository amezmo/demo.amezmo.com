#!/bin/bash


echo "Running composer install from ${APPLICATION_ROOT}";

composer \
    --no-ansi \
    --no-interaction \
    --optimize-autoloader \
    --no-progress \
    --no-dev \
    --profile \
    install

php artisan migrate --no-interaction --no-ansi --force;


echo "Running NPM install";
cd $APPLICATION_ROOT/realtime;
npm install;